<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.fpij.jitakyoei</groupId>
  <artifactId>sistema-de-judocas</artifactId>
  <version>1.0-SNAPSHOT</version>

  <name>sistema-de-judocas</name>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <maven.compiler.source>1.8</maven.compiler.source>
    <maven.compiler.target>1.8</maven.compiler.target>
  </properties>

  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>forms</groupId>
      <artifactId>forms</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>db4o</groupId>
      <artifactId>db4o</artifactId>
      <version>7.4.106.13438-java5</version>
    </dependency>

    <dependency>
      <groupId>jcalendar</groupId>
      <artifactId>jcalendar</artifactId>
      <version>1.3.3</version>
    </dependency>

    <dependency>
      <groupId>TableLayout-javadoc</groupId>
      <artifactId>TableLayout-javadoc</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>TableLayout-src</groupId>
      <artifactId>TableLayout-src</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>TableLayout</groupId>
      <artifactId>TableLayout</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>PgsLookAndFeel</groupId>
      <artifactId>PgsLookAndFeel</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>genesis.backport175</groupId>
      <artifactId>backport</artifactId>
      <version>175-1.1.RC1-snapshot</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>commons-beanutils</artifactId>
      <version>1.7</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>commons-digester</artifactId>
      <version>1.8</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.1</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>commons-validator</artifactId>
      <version>1.3.1</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>jakarta-oro</artifactId>
      <version>2.0.8</version>
    </dependency>

    <dependency>
      <groupId>genesis.commons</groupId>
      <artifactId>reusable-components</artifactId>
      <version>1.3.1</version>
    </dependency>

    <dependency>
      <groupId>genesis.script</groupId>
      <artifactId>bsh</artifactId>
      <version>2.0b4</version>
    </dependency>

    <dependency>
      <groupId>genesis.script</groupId>
      <artifactId>commons-el</artifactId>
      <version>1.0</version>
    </dependency>

    <dependency>
      <groupId>genesis.script</groupId>
      <artifactId>commons-jxpath</artifactId>
      <version>1.2</version>
    </dependency>

    <dependency>
      <groupId>genesis.script</groupId>
      <artifactId>jakarta-taglibs-standard</artifactId>
      <version>1.1.2</version>
    </dependency>

    <dependency>
      <groupId>genesis.script</groupId>
      <artifactId>js</artifactId>
      <version>1.6R5</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-annotation</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-aspect</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-aspect-annotated</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-client</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-client-swing</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-client-swt</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-client-thinlet</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-container</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-server</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-shared</artifactId>
      <version>3.2</version>
    </dependency>

    <dependency>
      <groupId>genesis</groupId>
      <artifactId>genesis-shared-annotated</artifactId>
      <version>3.2</version>
    </dependency>

  </dependencies>


  <build>

    <plugins>
      <!-- clean lifecycle, see
                https://maven.apache.org/ref/current/maven-core/lifecycles.html#clean_Lifecycle -->
      <plugin>
        <artifactId>maven-clean-plugin</artifactId>
        <version>3.1.0</version>
      </plugin>
      <!-- default lifecycle, jar packaging: see
      https://maven.apache.org/ref/current/maven-core/default-bindings.html#Plugin_bindings_for_jar_packaging -->
      <plugin>
        <artifactId>maven-resources-plugin</artifactId>
        <version>3.0.2</version>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.8.0</version>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.22.1</version>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.0.2</version>
      </plugin>
      <plugin>
        <artifactId>maven-install-plugin</artifactId>
        <version>2.5.2</version>
      </plugin>
      <plugin>
        <artifactId>maven-deploy-plugin</artifactId>
        <version>2.8.2</version>
      </plugin>
      <!-- site lifecycle, see
                https://maven.apache.org/ref/current/maven-core/lifecycles.html#site_Lifecycle -->
      <plugin>
        <artifactId>maven-site-plugin</artifactId>
        <version>3.7.1</version>
      </plugin>
      <plugin>
        <artifactId>maven-project-info-reports-plugin</artifactId>
        <version>3.0.0</version>
      </plugin>

      <plugin>
        <groupId>org.jacoco</groupId>
        <artifactId>jacoco-maven-plugin</artifactId>
        <version>0.8.11</version>
        <executions>
          <execution>
            <id>prepare-agent</id>
            <goals>
              <goal>prepare-agent</goal>
            </goals>
          </execution>
          <execution>
            <id>report</id>
            <phase>test</phase>
            <goals>
              <goal>report</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>

</project>